{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "jsx": "react",
    "sourceMap": true,
    // "outDir": "dist/cjs",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}